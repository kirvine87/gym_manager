<h1>Members</h1>
<a href="/members/new">Join</a>





<table>
  <tb>
  <tr>
    <th>Name</th>
    <th>Membership</th>
    <th>ID</th>
    <th>Details</th>
    <th>Quit</th>
  </tr>
  <% for member in @members %>
  <tr>
    <td><%= member.name() %></td>
    <td><%= member.membership() %></td>
    <td><%= member.id() %></td>
    <td><a href="/members/<%= member.id%>/edit">Edit</a></td>
    <td><form action="/members/<%= member.id %>/delete" method="POST">
    <input type="submit" value="Remove">
    </form>
    </td>
  </tr>
  <% end %>
  </tb>
</table>
