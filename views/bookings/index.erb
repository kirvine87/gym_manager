<h1>Bookings</h1>

<p><a href="/bookings/new">Book</a></p>
<table>
  <tr>
    <th>Class</th>
    <th>Name</th>
    <th>ID</th>
    <th>Cancel</th>
  </tr>
  <tb>
<% for booking in @bookings %>
<tr>
  <td><%= booking.gclass.name %> <%= booking.gclass.timec %></td>
  <td><%= booking.member.name %></td>
  <td><%= booking.member.id %></td>
  <td>
    <form action="/bookings/<%= booking.id %>/delete" method="POST">
    <input type="submit" value="Cancel">
    </form>
  </td>
</tr>
<% end %>
</tb>
</table>
